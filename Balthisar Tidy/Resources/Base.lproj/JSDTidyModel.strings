"specified input encoding (%s) does not match actual input encoding (%s)"	= "specified input encoding (%s) does not match actual input encoding (%s)";

/* ReportEncodingError */
"%s invalid character code %s"												= "%s invalid character code %s";
"%s invalid UTF-8 bytes (char. code %s)"									= "%s invalid UTF-8 bytes (char. code %s)";
"%s invalid UTF-16 surrogate pair (char. code %s)"							= "%s invalid UTF-16 surrogate pair (char. code %s)";
"%s invalid numeric character reference %s"									= "%s invalid numeric character reference %s";

/* ReportEntityError */
"entity \"%s\" doesn't end in ';'"											= "entity \"%s\" doesn't end in ';'";
"numeric character reference \"%s\" doesn't end in ';'"						= "numeric character reference \"%s\" doesn't end in ';'";
